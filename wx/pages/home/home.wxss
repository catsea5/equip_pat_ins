
.swiper-container{
    height: 130px;
  }
.swiper-image {
    width: 100%;
    height: 150px;
    border-top: 1rpx solid black;
}  
.plan {
    text-align: center;
    height: 30px;
    border-top: 1rpx solid black;
    
}
.title-text{
    color:#2b4b6b; 
    font-size: 20px; 
    font-weight:bold;
    
}
.plan-gird{
    display: flex; 
    flex-wrap: wrap; 
    border-left: 1px solid #efefef; 
    border-top: 1px solid #efefef;
}
.plan-list{
    height: 500rpx;
    width: 49%;
    text-align: center; 
    background-color: #2b4b6b;
    border-radius:20rpx; 
    box-shadow: 10rpx 10rpx rgb(163, 163, 182);
    border-bottom: 5rpx solid #efefef; 
    border-right: 1rpx solid #efefef;
}
.plan-list .title {
    font-size: 30rpx; 
    font-weight:bold;
    color:white;
}
.plan-list .start{
    float: left;
    margin-left: 4rpx; 
    font-weight:bold;
    color:white;
}
.plan-list .end{
    float: right;
    margin-right: 4rpx; 
    font-weight:bold;
    color:white;
}
.plan-list .startTime {
    float: left; 
    /* background-color: lightskyblue; */
    border-radius:20rpx;
    font-size: 25rpx;
    width: 140rpx;
    margin-left: 4rpx;
    color:white;

}
.plan-list .endTime{
    float: right; 
    /* background-color: lightskyblue; */
    border-radius:20rpx;
    font-size: 25rpx;
    width: 140rpx;
    margin-right: 4rpx;
    color:white;

}
.plan-list .btn{
    margin-bottom: 10rpx;
    border-radius: 15rpx;
    background-color: rgb(146, 197, 230);
    color: black;
}
.line{
    height: 3px;
    background-color: rgb(8, 8, 8);
}
  